<UserControl
    x:Class="ScreenMask.UserControls.EditorPlane"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ScreenMask.UserControls"
    mc:Ignorable="d"
    d:DesignHeight="450"
    d:DesignWidth="800">
    <Canvas>
        <local:RectEditor
            Canvas.Left="100"
            Canvas.Top="50"
            Width="200"
            Height="200" />
        <local:RectEditor
            Canvas.Left="400"
            Canvas.Top="500"
            Width="100"
            Height="50" />
        <local:RectEditor
            Canvas.Left="400"
            Canvas.Top="300"
            Width="10"
            Height="400" />
        <local:RectEditor
            Canvas.Left="200"
            Canvas.Top="150"
            Width="200"
            Height="200" />
    </Canvas>
</UserControl>
